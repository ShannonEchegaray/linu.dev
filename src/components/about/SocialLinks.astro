---
import TechsIcons from './TechsIcons.astro'

const socialLinks = [
  {
    name: 'github',
    url: 'https://github.com/linuxmobile'
  },
  {
    name: 'linkedin',
    url: 'https://www.linkedin.com/in/braiandiez/'
  },
  {
    name: 'twitter',
    url: 'https://twitter.com/linuxmobileok'
  }
]
---
{socialLinks.map((item) => (
  <a href={item.url} target='_blank' rel='noopener noreferrer' class='bg-rosepine-iris p-2 rounded-full'>
    <TechsIcons Tech={item.name} IconClass='w-6 text-surface' />
  </a>
))}
